package JavaPractise_InterviewPrograms;

public class ReverseNumberPractise1 {

	public static void main(String[] args) {

		int number=246;
		int temp,reverse=0;
		int a=number;
		while(number>0)
				{
			temp=number%10;
			reverse=number*10+temp;
			number=number/10;
				}
		System.out.println("Reverse is:"+number);
		
	}

}
