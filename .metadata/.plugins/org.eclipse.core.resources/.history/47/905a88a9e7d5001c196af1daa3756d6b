package FromSelenium10;

public class CustomCSS_Selector {

	public static void main(String[] args) {

		
//	----------------------------------------------------------------------
		
//		Syntax for id= #id
//		#username
//		input[id='username']
//		input[id='username'][type='email']
//	----------------------------------------------------------------------
		
//		for class -----> should start with dot(.)
//              syntax ----> .classname
		
//		.private-form__label-wrapper
	
//		div.private-form__label-wrapper ------> div= Html Tag
//		input.login-email
		
//		button#loginBtn
		
//	----------------------------------------
//		^ = charet symbol --------> Starting text
//		input[id^='username']
		
//	    --------------------
//		$ = dollar symbol --------> ending text
//		input[id$='username']
        	
//		input[id$='name']
//		input[id$='ame']
//		 --------------------
//		* = Asterisk symbol --------> contains text
//		 input[id*='name']
//		input[id*='username']		
		
	
	
	}

}
