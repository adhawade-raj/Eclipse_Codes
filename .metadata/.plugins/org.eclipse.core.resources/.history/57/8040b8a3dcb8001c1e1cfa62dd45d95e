package Practise;

class I
{
	void add(int...a)
	{
		System.out.println("no arguments" +a.length);
		int result =0;
		System.out.println(" argument list : ");
		for(int i=0; i<a.length; i++)
		{
			System.out.println(a[i]+" ");
			result=result+a[i];
		}
		
		System.out.println();
		System.out.println(" addition :   "+result);
		System.out.println("--------");
	}
	class test
	{
	 public static void main(String[] args)
	 {
		 I a=new I();
		 a.add();
		 a.add(10);
		 a.add(10,20);
		 a.add(10,20,30);
	 }
	}
}

