package Practise;

public class OnlyPractise {

	public static void main(String[] args) {
	
		final int i=5, j=10, k=15;
		
		switch(10)
		{
		case i :
			System.out.println("mon");
		break;
		
		case j : System.out.println("tue");
		break;
		
		case k: System.out.println("wed");
		break;
		
		
		
		
		}	

	}

}
