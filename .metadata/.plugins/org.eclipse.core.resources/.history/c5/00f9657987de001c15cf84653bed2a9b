package InterviewPrograms;

public class ReverseString {

	public static void main(String[] args) {

		String s= "MOM1";
		String reverse="";
		char[] a =s.toCharArray();
		
		System.out.println("Given String is:" +s);
		
		
		
		System.out.println("-------------");
