package Practise;

public class OnlyPractise {

	public static void main(String[] args) {
	
		
		int a=10;
		int b=0;
		int c=25;
		
		if(a>b)
		{
			if(a>c) {
			System.out.println("a is great");
		}
		System.out.println("c is great");
		}
		else if(b>c)
		{
			if(b>a)
			{
			System.out.println("b is great");
		}
			System.out.println("c is great");
		}
				
		
		
		
		
		
		
		
		

	}

}
